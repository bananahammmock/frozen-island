<h1>Sign up</h1>

<%= render partial: 'layouts/messages' %>

<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>
  <%= f.label :phone_number %>
  <%= f.number_field :phone_number %>
  <br>
  <%= f.submit 'Sign Up Yo' %>
<% end %>

