<%= image_tag current_user.image %>
<% if @user_themes_array %>
<%= "You have #{@user_themes_array.count} subscriptions" %>
<% end %>
<%= form_for @user do |u|%>
	<br>
  	<%= u.label :email %>
  	<%= u.text_field :email %>
  	<br>
	<%= u.label :phone_number %>
 	 <%= u.text_field :phone_number %>
  	<br>
  	<%= u.submit "Update" %>
<% end %>